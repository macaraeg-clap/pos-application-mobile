<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pos.ziaplex.com.posappmobile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity" android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".HomeActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".AboutActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".TransactionActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".BalanceActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".WithdrawalActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".PaymentActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".ConnectionActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".OperatorInfoActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".ConnectDeviceActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".TransactionFeeActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".TransactionSwipeCardActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".TransactionEnterPinActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".TransactionResultActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".PaymentDetailActivity" android:theme="@style/CustomTheme" />
        <activity android:name=".PaymentModeActivity" android:theme="@style/CustomTheme" />
    </application>

</manifest>